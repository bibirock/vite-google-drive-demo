{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "importHelpers": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "noEmit": true,
        "strict": true,
        "types": ["google.accounts", "node", "vite-plugin-pwa/client", "vite/client"],
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"]
        }
    },
    "vueCompilerOptions": {
        "plugins": ["@volar/vue-language-plugin-pug"]
    }
}
